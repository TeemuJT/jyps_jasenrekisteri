<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JYPS\User\Member" table="Member">
    <indexes>
      <index name="IDX_7748FF4EA221545F" columns="ParentMemberId"/>
      <index name="IDX_7748FF4E7AB83B07" columns="member_type_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="UNIQ_7748FF4E7597D3FE" columns="member_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstname" type="string" column="firstname" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surname" type="string" column="surname" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="streetAddress" type="string" column="street_address" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="postalCode" type="string" column="postal_code" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="60" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="email" type="string" column="email" length="60" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="refererPersonName" type="string" column="referer_person_name" length="60" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="magazinePreference" type="boolean" column="magazine_preference" nullable="false"/>
    <field name="invoicePreference" type="boolean" column="invoice_preference" nullable="false"/>
    <field name="memo" type="string" column="memo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="membershipStartDate" type="date" column="membership_start_date" nullable="false"/>
    <field name="membershipEndDate" type="date" column="membership_end_date" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="memberId" type="integer" column="member_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="telephone" type="string" column="telephone" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="gender" type="boolean" column="gender" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="selfcarePassword" type="string" column="selfcare_password" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="selfcarePasswordSalt" type="string" column="selfcare_password_salt" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="joinFormFreeword" type="string" column="join_form_freeword" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="mailingListYleinen" type="boolean" column="mailing_list_yleinen" nullable="false"/>
    <field name="birthYear" type="integer" column="birth_year" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="secondName" type="string" column="second_name" length="30" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="reminderSentDate" type="date" column="reminder_sent_date" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="nextMemberfeePaid" type="boolean" column="next_memberfee_paid" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <many-to-one field="memberType" target-entity="Memberfeeconfig" fetch="LAZY">
      <join-columns>
        <join-column name="member_type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="parentmemberid" target-entity="Member" fetch="LAZY">
      <join-columns>
        <join-column name="ParentMemberId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
