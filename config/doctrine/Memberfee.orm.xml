<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JYPS\User\Memberfee" table="MemberFee">
    <indexes>
      <index name="IDX_3F23B6387597D3FE" columns="member_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="feePeriod" type="integer" column="fee_period" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="feeAmountWithVat" type="decimal" column="fee_amount_with_vat" precision="10" scale="0" nullable="false"/>
    <field name="referenceNumber" type="string" column="reference_number" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dueDate" type="date" column="due_date" nullable="false"/>
    <field name="paid" type="boolean" column="paid" nullable="false"/>
    <field name="memo" type="string" column="memo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="emailSent" type="boolean" column="email_sent" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <many-to-one field="member" target-entity="Member" fetch="LAZY">
      <join-columns>
        <join-column name="member_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
