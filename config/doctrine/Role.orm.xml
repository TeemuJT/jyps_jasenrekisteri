<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JYPS\User\Role" table="Role">
    <unique-constraints>
      <unique-constraint name="UNIQ_F75B255457698A6A" columns="role"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="role" type="string" column="role" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="user" target-entity="User" mapped-by="role" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
