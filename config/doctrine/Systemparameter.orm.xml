<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JYPS\User\Systemparameter" table="SystemParameter">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="key" type="string" column="key" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="intValue" type="integer" column="int_value" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="stringValue" type="string" column="string_value" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="decValue" type="decimal" column="dec_value" precision="10" scale="0" nullable="false"/>
    <field name="booleanValue" type="boolean" column="boolean_value" nullable="false"/>
  </entity>
</doctrine-mapping>
