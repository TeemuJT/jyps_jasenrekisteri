<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="JYPS\User\Memberfeeconfig" table="MemberFeeConfig">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="memberfeeName" type="string" column="memberfee_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="memberfeeAmount" type="decimal" column="memberfee_amount" precision="10" scale="0" nullable="false"/>
    <field name="validFrom" type="date" column="valid_from" nullable="false"/>
    <field name="validTo" type="date" column="valid_to" nullable="false"/>
    <field name="memberType" type="integer" column="member_type" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createfees" type="string" column="createfees" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="showOnJoinForm" type="boolean" column="show_on_join_form" nullable="false"/>
    <field name="campaignFee" type="boolean" column="campaign_fee" nullable="false"/>
    <field name="realMembertype" type="integer" column="real_membertype" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="showAmount" type="boolean" column="show_amount" nullable="false"/>
    <field name="changeAllowedTo" type="boolean" column="change_allowed_to" nullable="false"/>
    <field name="changeAllowedFrom" type="boolean" column="change_allowed_from" nullable="false"/>
  </entity>
</doctrine-mapping>
